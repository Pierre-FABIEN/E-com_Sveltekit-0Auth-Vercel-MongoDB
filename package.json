    "@sveltejs/adapter-vercel": "latest",
    "@sveltejs/kit": "latest",
    "@types/bcryptjs": "latest",
    "@types/jsonwebtoken": "latest",
    "@typescript-eslint/eslint-plugin": "latest",
    "@typescript-eslint/parser": "latest",
    "autoprefixer": "latest",
    "chokidar-cli": "latest",
    "concurrently": "latest",
    "eslint": "latest",
    "eslint-config-prettier": "latest",
    "eslint-plugin-prettier": "latest",
    "gsap": "latest",
    "prettier": "latest",
    "prettier-plugin-svelte": "latest",
    "prisma": "latest",
    "sass": "latest",
    "smooth-scrollbar": "latest",
    "svelte": "latest",
    "svelte-check": "latest",
    "svelte-preprocess": "latest",
    "tslib": "latest",
    "typescript": "latest",
    "vite-plugin-sass": "latest"
  },
  "type": "module",
  "dependencies": {
    "@auth/sveltekit": "latest",
    "@sveltejs/vite-plugin-svelte": "latest",
    "@tsconfig/svelte": "latest",
    "@tweakpane/core": "latest",
    "@types/three": "latest",
    "bits-ui": "latest",
    "clsx": "latest",

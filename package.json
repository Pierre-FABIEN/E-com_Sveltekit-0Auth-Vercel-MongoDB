{
  "name": "boilersvelte",
  "version": "0.0.1",
  "scripts": {
    "dev": "concurrently \"vite dev\" \"npm run prettier:watch\"",
    "prettier:watch": "chokidar \"src/**/*.{js,ts,svelte,scss}\" -c \"npm run prettier:single -- {path}\"",
    "prettier:single": "prettier --write",
    "prettier": "prettier --write \"src/**/*.{js,ts,svelte,scss}\"",
    "build": "prisma generate && vite build",
    "preview": "vite preview",
    "test": "playwright test",
    "test:unit": "vitest"
  },
  "browserslist": [
    "> 5%"
  ],
  "devDependencies": {
    "@playwright/test": "latest",
    "@prisma/client": "latest",
    "@rollup/plugin-terser": "latest",
